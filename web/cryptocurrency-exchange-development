} else {
$("#contactus .text_type_exchange_div").css('display','none');
}
$( "#contactus .text_type_exchange" ).html('');
jQuery.each( arr, function( i, val ) {
$( "#contactus .text_type_exchange" ).append( "<option value='"+val+"'>"+val+"</option>" );
});

